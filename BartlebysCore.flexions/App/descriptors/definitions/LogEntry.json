{
  "name": "LogEntry",
  "definition": {
    "explicitType": "Model",
    "description": "Bartleby's Core: A single Log entry",
    "properties": {
      "counter": {
        "type": "integer",
        "description": "The print entry counter",
        "default": -1,
        "supervisable": false,
        "dynamic" : true
      },
      "line": {
        "type": "integer",
        "description": "The referent line",
        "default": -1,
        "supervisable": false,
        "dynamic" : true
      },
      "elapsedTime": {
        "type": "double",
        "description": "The elasped duration",
        "default": -1,
        "supervisable": false,
        "dynamic" : true
      },
      "message": {
        "type": "string",
        "description": "the message",
        "default": "$Default.NO_MESSAGE",
        "supervisable": false,
        "dynamic" : true
      },
      "file": {
        "type": "string",
        "description": "the file",
        "default": "$Default.NO_FILE",
        "supervisable": false,
        "dynamic" : true
      },
      "function": {
        "type": "string",
        "description": "the function",
        "default": "$Default.NO_FUNCTION",
        "supervisable": false,
        "dynamic" : true
      },
      "category": {
        "type": "enum",
        "instanceOf": "string",
        "enumPreciseType": "LogEntry.Category",
        "description": "the log category",
        "enum": [
          "standard",
          "temporary",
          "critical"
        ],
        "default": ".standard",
        "supervisable":false,
        "dynamic" : true
      },
      "decorative": {
        "type": "boolean",
        "description": "Is the entry decorative or significant? decoration includes separators, etc...",
        "default": "false",
        "supervisable": false,
        "dynamic" : true
      },
      "runUID": {
        "type": "string",
        "description": "A session runUID identifyer",
        "default": "$Session.runUID",
        "supervisable": false,
        "dynamic" : true
      }
    },
    "metadata": {
      "urdMode": false,
      "persistsLocallyOnlyInMemory": false,
      "persistsDistantly": false,
      "undoable": false
    }
  }
}
